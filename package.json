{
  "name": "part-virtual-scroll",
  "version": "1.0.1",
  "description": "虚拟列表滚动",
  "main": "dist/part-scroll.js",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:serve": "http-server ./dist -p 8080 -s -c-1 -o",
    "dev:reload": "livereload  ./dist",
    "dev:build": "rollup -c rollup.config.js --watch",
    "umd": "rollup -c rollup.config.umd.js",
    "eslint": "eslint src/",
    "test": "jest --config=./jest.config.js",
    "json": "json-server ./mock/data.cjs --watch"
  },
  "author": "Menchem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Menchem/part-virtual-scroll.git"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.1",
    "jest": "^24.7.1",
    "json-server": "^0.17.2",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-simple-vars": "^5.0.2",
    "rollup": "^3.17.3",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "http-server": "^14.1.1",
    "livereload": "^0.9.3"
  }
}
